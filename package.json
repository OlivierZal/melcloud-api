{
  "name": "@olivierzal/melcloud-api",
  "version": "21.0.0",
  "description": "MELCloud API for Node.js",
  "keywords": [
    "melcloud",
    "api"
  ],
  "homepage": "https://github.com/OlivierZal/melcloud-api#readme",
  "bugs": {
    "url": "https://github.com/OlivierZal/melcloud-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlivierZal/melcloud-api.git"
  },
  "license": "ISC",
  "author": "Olivier Zalmanski",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/main.js"
  },
  "types": "dist/main.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "docs": "typedoc && npm run format:docs",
    "format": "prettier . --check",
    "format:docs": "prettier README.md --ignore-path --write",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "npm run docs && npm run lint:fix && npm run format:fix && npm run build"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "core-js": "^3.48.0",
    "https": "^1.0.0",
    "luxon": "^3.7.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@eslint/markdown": "^7.5.1",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.3.0",
    "@typescript/native-preview": "^7.0.0-dev.20260219.1",
    "eslint": "10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-package-json": "^0.88.3",
    "eslint-plugin-perfectionist": "^5.6.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "eslint-plugin-yml": "^3.2.2",
    "jsonc-eslint-parser": "^3.1.0",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "typedoc": "^0.28.17",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
